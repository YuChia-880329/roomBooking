<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="springboot.dao.model.SectionMapper">
	
	<select id="queryAll" resultType="springboot.bean.model.Section">
		SELECT
		    CODE,
		    NAME
		FROM
    		SECTION
	</select>
</mapper>